{
  "name": "vscode-devfile-sync-extension",
  "version": "1.0.0",
  "description": "A VS Code extension for synchronizing devfile.yaml and devcontainer files",
  "author": "sandrociceros and Github Copilot :P",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "package": "vsce package",
